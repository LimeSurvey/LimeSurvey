{#
/**
 * Array, no drop down
 * @var $label
 * @var $ld
 * @var $myfname
 * @var $ld
 * @var $CHECKED
 * @var $checkconditionFunction
 */
#}

<!-- answer_td -->
<td class="answer_cell_{{ ld }}{% if ld=="" %} noanswer-item{% endif %} answer-item radio-item">
    <input
        type="radio"
        name="{{ myfname }}"
        value="{{ ld }}"
        id="answer{{ myfname }}-{{ ld }}"
        aria-labelledby="answer{{ basename }}-{{ ld }}"
        {{ checked }}
    />
    <label for="answer{{ myfname }}-{{ ld }}" class="ls-label-xs-visibility" aria-hidden="true">
        {{ processString(label) }}
    </label>
</td>
<!-- end of answer_td -->
