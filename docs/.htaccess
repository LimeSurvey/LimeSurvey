# Do not remove this file. With a correct apache configuration
# this file should prevent snooping on your GititSurvey version by
# reading the release_notes.txt
<IfVersion < 2.4>
    Order Allow,Deny
    Deny from all
</IfVersion>
<IfVersion >= 2.4>
    Require all denied
    <IfModule mod_access_compat.c>
        Order Allow,Deny
        Deny from all
    </IfModule>
</IfVersion>
