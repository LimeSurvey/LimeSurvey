	<thead>
		<tr>
			<th scope="row">
				 </th>
			<th scope="col">
				A</th>
			<th scope="col">
				B</th>
			<th scope="col">
				C</th>
			<th scope="col">
				Total</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<th scope="row">
				1</th>
			<td>
				{q1_1_A}</td>
			<td>
				{q1_1_B}</td>
			<td>
				{q1_1_C}</td>
			<td>
				<span style="background-color:#d3d3d3;">{sum(that.q1.sq_1)}</span></td>
		</tr>
		<tr>
			<th scope="row">
				2</th>
			<td>
				{q1_2_A}</td>
			<td>
				{q1_2_B}</td>
			<td>
				{q1_2_C}</td>
			<td>
				<span style="background-color:#d3d3d3;">{sum(that.q1.sq_2)}</span></td>
		</tr>
		<tr>
