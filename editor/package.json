  "scripts": {
    "prestart": "npm run collect-translations",
    "dev": "rsbuild dev",
    "start": "export REACT_APP_COMMIT_HASH=$(git rev-parse HEAD) && rsbuild dev",
    "start-https": "HTTPS=true rsbuild preview",
    "build": "export REACT_APP_COMMIT_HASH=$(git rev-parse HEAD) && rsbuild build",
    "build-https": "HTTPS=true rsbuild build",
    "build-demo": "PUBLIC_URL=/ REACT_APP_DEV_MODE= REACT_APP_DEMO_MODE=true rsbuild build",
    "postbuild": "node scripts/postbuild.js",
    "test": "REACT_APP_DEMO_MODE=true jest --ci",
    "storybook": "REACT_APP_DEV_MODE=true STORYBOOK_DEV=true storybook dev -p 6006",
    "storybook:win": "set STORYBOOK_DEV=true && storybook dev -p 6006",
    "build-storybook": "REACT_APP_DEV_MODE=true STORYBOOK_DEV=true storybook build",
    "test-storybook": "test-storybook --maxWorkers 1 --browsers chromium -- --testTimeout=60000",
    "test-storybook-all-browsers": "test-storybook --maxWorkers 1 --browsers firefox chromium webkit -- --testTimeout=60000",
    "test-storybook:coverage": "test-storybook -- --coverage  --testTimeout=60000",
    "ci:danger": "npx danger ci",
    "eslint": "eslint src --ext .js --max-warnings 0",
    "prettier:check": "prettier --check src",
    "prettier:format": "prettier --write src",
    "prepare": "husky",
    "collect-translations": "node src/i18n/scripts/collectTranslationStrings.js"
  },
