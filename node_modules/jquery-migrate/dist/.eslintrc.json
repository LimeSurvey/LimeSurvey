{
	"root": true,

	"extends": "../src/.eslintrc.json",

	"parserOptions": {
		"ecmaVersion": 5,
		"sourceType": "script"
	},

	"globals": {
		"define": false,
		"module": true,
		"Proxy": false,
		"Reflect": false
	},

	"rules": {
		// That is okay for the built version
		"no-multiple-empty-lines": "off",
		"one-var": "off"
	}
}
