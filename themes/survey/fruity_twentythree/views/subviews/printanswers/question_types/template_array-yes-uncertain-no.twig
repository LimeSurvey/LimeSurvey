 {% extends "./subviews/printanswers/printanswers_question.twig" %}
 {% block question_answer %}
 <div class="col-lg-8">
    {% for subquestionId, subquestion in question.subquestions %}
        <div class="row">
            <div class="col-lg-4 text-end">
                {% if subquestion.answervalue is same as("Y") %}
                    {{ gT("Yes") }} <i class="ri-checkbox-line"></i>
                {% elseif subquestion.answervalue is same as("N") %}
                    {{ gT("No") }} <i class="ri-checkbox-blank-line"></i>
                {% elseif subquestion.answervalue is same as("U") %}
                    {{ gT("Uncertain") }} <i class="ri-checkbox-indeterminate-line"></i>
                {% else %}

                {% endif %}
            </div>
            <div class="col-lg-8 text-start">
                <b>{{subquestion.question}} ({{subquestion.qid}})</b>
            </div>
        </div>
    {% endfor %}
</div>
{% endblock %}
